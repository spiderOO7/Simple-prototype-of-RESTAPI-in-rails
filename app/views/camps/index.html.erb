<p id="notice"><%= notice %></p>

<h1>Camps</h1>

<table>
  <thead>
    <tr>
      <th>Did</th>
      <th>Model</th>
      <th>Name</th>
      <th>Sdata</th>
      <th>Edate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @camps.each do |camp| %>
      <tr>
        <td><%= camp.did %></td>
        <td><%= camp.model %></td>
        <td><%= camp.name %></td>
        <td><%= camp.sdata %></td>
        <td><%= camp.edate %></td>
        <td><%= link_to 'Show', camp %></td>
        <td><%= link_to 'Edit', edit_camp_path(camp) %></td>
        <td><%= link_to 'Destroy', camp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Camp', new_camp_path %>
